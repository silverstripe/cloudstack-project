{
	"name": "silverstripe/cloudstack-project",
	"description": "The SilverStripe Platform base project / installer for CWP CloudStacks.",
	"require": {
		"php": ">=5.6",
		"silverstripe/cms": "^3.6",
		"silverstripe/framework": "^3.6",
		"silverstripe/dynamodb": "^3.0",
		"silverstripe/crontask": "^1.1",
		"cwp/starter-theme": "^1.1",
		"cwp/watea-theme": "^1.1"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~4.8"
	},
	"config": {
		"process-timeout": 600
	},
	"repositories": [
		{
		"type": "composer",
		"url": "https://packages.cwp.govt.nz/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true
}
